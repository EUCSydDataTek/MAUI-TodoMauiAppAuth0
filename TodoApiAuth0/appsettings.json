{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "TodoDBConnectionString": "Data Source=Todo.db"
  },

  // * Used by Auth0 *
